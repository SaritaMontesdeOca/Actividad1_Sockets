CC = g++
CFLAG = -Wall
LIBS = 

#Definici贸n de los archivos .o Requeridos
OBJS = web_server.o 

#Definici贸n del nombre del programa
NAME = web_server

#Creaci贸n de Regla por Default
all: ${NAME}

${NAME}: ${OBJS}
